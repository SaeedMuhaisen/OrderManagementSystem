{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/Navigation":["src/Navigation/index.ts"],
      "@/Redux":["src/redux/index.ts"],
      "@/Types":["src/Types/index.ts"],
      "@/Features/Customer/OrderHistory":["src/Features/Buyer/OrderHistory/index.ts"],
      "@/Features/Customer/ShoppingCart":["src/Features/Buyer/ShoppingCart/index.ts"],
      "@/Features/Customer/Store":["src/Features/Buyer/Store/index.ts"],
      "@/Features/Seller/Orders":["src/Features/Seller/Orders/index.ts"],
      "@/Features/Seller/Product":["src/Features/Seller/Product/index.ts"],
      "@/Features/Common/NavigationBar":["src/Features/Common/NavigationBar/index.ts"],
      "@/Notifications":["src/Features/Common/Notifications/index.ts"],
      "@/Features/Common/Componenets":["src/Features/Common/components/index.ts"],
      "@/Authentication":["src/Features/Common/Authentication/index.ts"],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src"
  ]
}